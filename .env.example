# Claude API Configuration
ANTHROPIC_API_KEY=your_api_key_here

# Model Configuration
MODEL_PRIMARY=anthropic/claude-haiku-4-5
MODEL_FALLBACK=anthropic/claude-sonnet-4-20250514
MODEL_CRITICAL=anthropic/claude-opus-4-5

# Cache Configuration
CACHE_TTL=3600000
CACHE_MAX_SIZE=1000

# Batch Configuration
BATCH_MAX_SIZE=100000
BATCH_MAX_WAIT_TIME=86400000

# Optimization Flags
ENABLE_DYNAMIC_ROUTING=true
ENABLE_PROMPT_CACHING=true
ENABLE_RESPONSE_CACHING=true
ENABLE_BATCH_PROCESSING=true
ENABLE_COST_MONITORING=true

# Logging
LOG_LEVEL=info
LOG_COSTS=true

# Benchmark Configuration
BENCHMARK_ITERATIONS=100
BENCHMARK_VERBOSE=false